entity Customer{
	name String required,
    firstName String required,
    birthDate Instant required,
    sex Sex required,
    phone Integer required,
    email String,
    objective Goal,
    observations String,
    creationDate Instant,
    isActive Boolean
}

enum Sex {
    MASCULINO, FEMENINO
}

enum Goal {
    MUSCULACION, FITNESS, CULTURISMO, DEPORTE_CONTACTO
}